    "{\n",
    " \"cells\": [\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"# Paths so we can import both the root-level helper and the local misc\\n\",\n",
    "    \"from pathlib import Path\\n\",\n",
    "    \"import sys\\n\",\n",
    "    \"import numpy as np\\n\",\n",
    "    \"\\n\",\n",
    "    \"CWD = Path.cwd()  # lab_value_iteration/\\n\",\n",
    "    \"ROOT = CWD.parent\\n\",\n",
    "    \"if str(ROOT) not in sys.path: sys.path.append(str(ROOT))\\n\",\n",
    "    \"if str(CWD) not in sys.path: sys.path.append(str(CWD))\\n\",\n",
    "    \"\\n\",\n",
    "    \"from helperff import make_env, QLParams, run_q_learning_experiment, q_to_v, greedy_policy_from_q\\n\",\n",
    "    \"from lab_value_iteration import misc  # local misc.py\\n\",\n",
    "    \"import helperff\"\n",
    "   ],\n",
    "   \"id\": \"7bda57220766af7f\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"source\": \"# Q-Learning Integration\",\n",
    "   \"id\": \"6f106dafdf4653a0\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"# Map/env settings â€” keep consistent with your MC notebooks when comparing\\n\",\n",
    "    \"MAP_SIZE = 11\\n\",\n",
    "    \"P_FROZEN = 0.85\\n\",\n",
    "    \"SEED = 69\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Two envs: one for training (fast), one for pretty rendering/plots if needed\\n\",\n",
    "    \"env_train = make_env(is_slippery=False, map_size=MAP_SIZE, proba_frozen=P_FROZEN, seed=SEED, render_mode=None)\\n\",\n",
    "    \"env_viz   = make_env(is_slippery=False, map_size=MAP_SIZE, proba_frozen=P_FROZEN, seed=SEED, render_mode=\\\"rgb_array\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Q-learning hyperparameters\\n\",\n",
    "    \"ql_params = QLParams(\\n\",\n",
    "    \"    episodes=25000,\\n\",\n",
    "    \"    max_steps=200,\\n\",\n",
    "    \"    alpha=0.1,\\n\",\n",
    "    \"    gamma=0.99,\\n\",\n",
    "    \"    epsilon_start=1.0,\\n\",\n",
    "    \"    epsilon_min=0.05,\\n\",\n",
    "    \"    epsilon_decay=0.999,\\n\",\n",
    "    \"    seed=SEED,\\n\",\n",
    "    \"    savefig_folder=\\\"figures\\\"\\n\",\n",
    "    \")\\n\"\n",
    "   ],\n",
    "   \"id\": \"fd252be9393995cb\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"source\": \"## Train Q-learning agent\",\n",
    "   \"id\": \"90f0715751431d89\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"qtable, rewards_q, lengths_q = run_q_learning_experiment(env_train, ql_params)\\n\",\n",
    "    \"V_q = q_to_v(qtable)\\n\",\n",
    "    \"policy_q = greedy_policy_from_q(qtable)\\n\"\n",
    "   ],\n",
    "   \"id\": \"9945a5ad8e60b003\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"source\": \"## Visualize Q-learning results\",\n",
    "   \"id\": \"7424291ec1616497\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"# Learning curve (reward vs episodes)\\n\",\n",
    "    \"misc.plot_learning_curve(rewards_q, label=\\\"Q-Learning (shaped rewards)\\\", smooth=200)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Trajectory lengths vs episodes\\n\",\n",
    "    \"misc.plot_lengths_curve(lengths_q, label=\\\"Q-Learning\\\", smooth=200)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Value function from Q (V(s) = max_a Q)\\n\",\n",
    "    \"misc.plot_value_heatmap(V_q, MAP_SIZE, title=\\\"Q-Learning: V(s)\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"state, _ = env_viz.reset(seed=SEED)  # <-- REQUIRED before .render()\\n\",\n",
    "    \"last_frame = env_viz.render()        # now valid\\n\",\n",
    "    \"\\n\",\n",
    "    \"# rollout to the end using greedy policy from Q\\n\",\n",
    "    \"policy = np.argmax(qtable, axis=1)\\n\",\n",
    "    \"for _ in range(ql_params.max_steps):\\n\",\n",
    "    \"    a = int(policy[state])\\n\",\n",
    "    \"    state, _, terminated, truncated, _ = env_viz.step(a)\\n\",\n",
    "    \"    last_frame = env_viz.render()\\n\",\n",
    "    \"    if terminated or truncated:\\n\",\n",
    "    \"        break\\n\",\n",
    "    \"\\n\",\n",
    "    \"# now plot\\n\",\n",
    "    \"misc.plot_q_values_map(qtable, env_viz, MAP_SIZE, params=ql_params, img_label=\\\"Q-Learning\\\")\\n\"\n",
    "   ],\n",
    "   \"id\": \"472f31d2938efe1b\"\n",
    "  }\n",
    " ],\n",
